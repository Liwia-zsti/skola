    <script>
        let test = true
        do{
            liczba1 = parseInt(prompt("Podaj 1 liczbę: "))
            liczba2 = parseInt(prompt("Podaj 2 liczbę: "))
            if (isNaN(liczba1) == true || inNan(liczba2) == true){
        prompt("Nie podano prawidłowych wartości :9")
            test = true
        }
        else (test == true)
    }
    while(test == true)
    let test = true

    </script>
